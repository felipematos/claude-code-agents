{
  "name": "claude-agents-dashboard",
  "version": "1.0.0",
  "description": "Local web dashboard for Claude Code Agents management",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm run install-all && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "body-parser": "^1.20.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "claude",
    "agents",
    "dashboard",
    "project-management",
    "task-management"
  ],
  "author": "Claude Code Agents",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}